# ===========================================
# 生产环境配置
# ===========================================

# 应用信息
VITE_APP_TITLE=夜色视频
VITE_APP_VERSION=1.0.0

# API 配置 - 生产环境
VITE_API_BASE_URL=https://api.yese.tv/api
VITE_API_TIMEOUT=10000

# 开发工具 - 生产环境全部关闭
VITE_ENABLE_MSW=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_DEVTOOLS=false

# 第三方服务 - 正式环境
VITE_PAYMENT_ALIPAY_APP_ID=prod_alipay_app_id
VITE_PAYMENT_WECHAT_APP_ID=prod_wechat_app_id

# 上传配置 - 生产环境限制更严格
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif

# 视频配置
VITE_VIDEO_DEFAULT_QUALITY=1080p
VITE_VIDEO_AUTOPLAY=false

# 生产环境启用分析
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ID=prod_analytics_id
