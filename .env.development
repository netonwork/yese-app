# ===========================================
# 开发环境配置
# ===========================================

# 应用信息
VITE_APP_TITLE=夜色视频 (开发版)
VITE_APP_VERSION=1.0.0-dev

# API 配置 - 开发环境
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000

# 开发工具 - 全部启用
VITE_ENABLE_MSW=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEVTOOLS=true

# 第三方服务 - 测试环境
VITE_PAYMENT_ALIPAY_APP_ID=test_alipay_app_id
VITE_PAYMENT_WECHAT_APP_ID=test_wechat_app_id

# 上传配置 - 开发环境较宽松
VITE_UPLOAD_MAX_SIZE=52428800
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/webm

# 视频配置
VITE_VIDEO_DEFAULT_QUALITY=720p
VITE_VIDEO_AUTOPLAY=false

# 开发环境不启用分析
VITE_ENABLE_ANALYTICS=false
