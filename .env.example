# ===========================================
# 环境变量配置模板
# 复制此文件为 .env 并修改相应的值
# ===========================================

# 应用基本信息
VITE_APP_TITLE=夜色视频
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=专业的视频观看平台

# API 配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# 开发工具配置
VITE_ENABLE_MSW=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEVTOOLS=true

# 第三方服务配置
VITE_PAYMENT_ALIPAY_APP_ID=your_alipay_app_id_here
VITE_PAYMENT_WECHAT_APP_ID=your_wechat_app_id_here

# 上传配置
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,video/mp4

# 视频播放配置
VITE_VIDEO_DEFAULT_QUALITY=720p
VITE_VIDEO_AUTOPLAY=false

# 分析和监控
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=your_analytics_id_here
